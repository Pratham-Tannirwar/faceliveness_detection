{
  "name": "facelive",
  "version": "1.0.0",
  "description": "Real-time face detection and live streaming application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node setup.js",
    "setup-db": "node scripts/setup-database.js",
    "setup-db-complete": "node scripts/setup-database-complete.js",
    "init-db": "node scripts/init-db-simple.js",
    "test-db": "node scripts/test-database.js",
    "dev:frontend": "cd frontend && cross-env PORT=3001 npm start",
    "dev:backend": "nodemon server.js",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "start:all": "concurrently \"npm start\" \"npm run dev:frontend\""
  },
  "keywords": [
    "face-detection",
    "live-streaming",
    "camera",
    "real-time"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "face-api.js": "^0.20.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0-rc.4",
    "pg": "^8.11.3",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
